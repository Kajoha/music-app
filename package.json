{
  "name": "music-app",
  "version": "2.0.0",
  "description": "music app",
  "scripts": {
    "sass": "node-sass -w sass -o public/css",
    "sass-build": "node-sass sass -o public/css --output-style compressed",
    "dev-server": "live-server --watch=\"index.html,sass,js\"",
    "eslint": "eslint js",
    "dev": "npm run sass-build && concurrently \"npm run sass\" \"npm run dev-server\" \"esw -w js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "start:dev": "nodemon js/app.js"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-watch": "^7.0.0",
    "live-server": "^1.2.1",
    "livereload": "^0.9.1",
    "node-sass": "^4.14.1"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kajoha/music-app.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/Kajoha/music-app/issues"
  },
  "homepage": "https://github.com/Kajoha/music-app#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.8",
    "mongoose": "^5.13.3",
    "nodemon": "^2.0.12",
    "pug": "^3.0.2"
  }
}
